        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "description": "Title of the assessment."
          },
          "description": {
            "type": "string",
            "description": "Detailed description of the assessment."
          },
          "createdBy": {
            "type": "string",
            "description": "Identifier of the user creating the assessment (UUID).",
            "format": "uuid"
          },
          "frameworkId": {
            "type": "string",
            "description": "Identifier of the framework this assessment uses (UUID).",
            "format": "uuid"
          },
          "assessmentDepthId": {
            "type": "string",
            "description": "Identifier of the chosen assessment depth",
            "format": "uuid"
          },
          "assessmentScoringId": {
            "type": "string",
            "description": "Identifier of the chosen assessment scoring model",
            "format": "uuid"
          }
        },
        "additionalProperties": false,
        "description": "Request body used to create a new assessment."
      },
      "FrameworkOptionsDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "assessmentDepths": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AssessmentDepthOptionsDto"
            }
          }
        },
        "additionalProperties": false
      },
