          "name": {
            "type": "string"
          },
          "assessmentScorings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AssessmentScoringOptionsDto"
            }
          }
        },
        "additionalProperties": false
      },
      "AssessmentDepthResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "frameworkId": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
